{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "es5",
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "src/*": ["./src/*"],
      "assets/*": ["./src/assets/*"],
      "components/*": ["./src/components/*"],
      "hooks/*": ["./src/hooks/*"],
      "libs/*": ["./src/libs/*"],
      "styles/*": ["./src/styles/*"],
      "types/*": ["./src/types/*"]
    }
  }
}
